 //array
let fruits=['apple','mango',23,true]
console.log(fruits)
console.log(fruits[0])//accssed particular element
fruits.push(false)//adding
fruits.unshift('graphs')//adding first
fruits.pop()//delete last element
fruits.shift(fruits)//delete first element
console.log(fruits)
fruits.shift(fruits[0])//delete particular element
console.log(fruits) 

